<div metal:use-macro="load: base.pt">
    <tal:block metal:fill-slot="content">
        <h1>${name}</h1>
        <p>login: <b class="byline">${username}</b>.</p>
        <p>email: <b class="byline">${email}</b>.</p>
        <p>account created at: <b class="byline">${created_at}</b>.</p>
        <p>account verified at: <b class="byline">${verified_at}</b>.</p>
    </tal:block>
</div>
